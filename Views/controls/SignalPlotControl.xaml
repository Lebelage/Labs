<UserControl x:Class="Lab1.Views.controls.SignalPlotControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:scottplot="clr-namespace:ScottPlot;assembly=ScottPlot.WPF"
             xmlns:local="clr-namespace:Lab1.Views.controls"
             mc:Ignorable="d"
             DataContext="{Binding SignalPlotVM, Source={StaticResource Locator}}"
             d:DesignHeight="450" d:DesignWidth="800">
    <ContentControl Content="{Binding Plot, Mode=OneTime}"/>
</UserControl>
